{
  "name": "vite-react-app",
  "version": "0.9.0",
  "scripts": {
    "dev": "cross-env ENV_TARGET=development vite",
    "build": "cross-env vite build",
    "publish": "cross-env TARGET=github vite build",
    "build:qa": "cross-env ENV_TARGET=qa vite build",
    "build:uat": "cross-env ENV_TARGET=uat vite build",
    "serve": "cross-env vite preview",
    "cspell": "cspell 'src/**/*.{jsx,jsx,scss}'",
    "prettier": "prettier --write .",
    "lint": "eslint --fix --ext .jsx,js .",
    "test": "jest --passWithNoTests",
    "build:icon": "node ./iconfont.config.js",
    "preview:icon": "npm run build:icon && npx serve ./src/assets/fonts",
    "stylelint": "stylelint **/*.{css,scss,less,styl} --fix"
  },
  "dependencies": {
    "@loadable/component": "^5.14.1",
    "axios": "^0.21.1",
    "clsx": "^1.1.1",
    "dayjs": "^1.10.4",
    "lodash": "^4.17.20",
    "prop-types": "^15.7.2",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-router-config": "^5.1.1",
    "react-router-dom": "^5.2.0",
    "recoil": "^0.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/eslint-parser": "^7.12.17",
    "@babel/plugin-transform-runtime": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-react": "^7.12.13",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@vitejs/plugin-react-refresh": "^1.1.0",
    "autoprefixer": "^10.2.4",
    "babel-plugin-bundled-import-meta": "^0.3.2",
    "cross-env": "^7.0.3",
    "cspell": "^5.2.4",
    "cssnano": "^4.1.10",
    "esbuild": "^0.8.50",
    "esbuild-jest": "^0.4.0",
    "eslint": "^7.20.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.5",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^5.1.0",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "postcss": "^8.2.6",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "stylelint": "^13.10.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "svgtofont": "^3.8.2",
    "vite": "^2.0.1",
    "vite-babel-plugin": "0.0.2",
    "vite-plugin-eslint": "^1.0.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  }
}
